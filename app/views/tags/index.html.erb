<h1>All Tags</h1>
<ul id="tags">
    <% @tags.each do |tag| %>
        <li>
            <%= link_to tag.name, tag_path(tag)%>
            <%= link_to "Delete Tag", tag_path(tag), method: :delete,
            data: {confirm: "Delete Tag ?"}%>
        </li>
    <% end %>

    <%= link_to "Back to Articles", articles_path%>
</ul>